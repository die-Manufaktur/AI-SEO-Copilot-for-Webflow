  "private": true,
  "main": "client/src/main.tsx",
  "type": "module",
  "scripts": {
    "check": "tsc",
    "test": "vitest run --coverage",
    "test:watch": "vitest --coverage",
    "test:ui": "vitest --ui --coverage",
    "test:bundle": "vitest run client/src/lib/bundle-validation.test.ts",
    "dev": "concurrently -r \"vite --cors --force\" \"vite build --mode development && webflow extension serve\" \"npx wrangler dev ./workers/index.ts --port 8787\"",
    "build": "vite build && webflow extension bundle",
    "deploy:worker": "npx wrangler deploy workers/index.ts --env production",
    "kill-dev": "kill-dev-servers.bat",
    "prepare": "husky",
    "release": "semantic-release",
    "release:dry": "semantic-release --dry-run",
    "version:update": "node scripts/update-versions.js",
    "changelog": "conventional-changelog -p conventionalcommits -r 2 -i CHANGELOG.md -s",
    "commit": "git-cz --sign"
  },
