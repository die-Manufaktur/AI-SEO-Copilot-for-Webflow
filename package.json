{
  "name": "ai-seo-copilot",
  "version": "2.2.0",
  "private": true,
  "main": "client/src/main.tsx",
  "type": "module",
  "scripts": {
    "build": "vite build && esbuild workers/index.ts --platform=browser --packages=external --bundle --external:ip --external:ip-cidr --format=esm --outdir=public && webflow extension bundle",
    "dev": "concurrently -r \"vite --force\" \"webflow extension serve\"",
    "start": "NODE_ENV=production node public/index.js",
    "check": "tsc",
    "dev:worker": "npx wrangler dev workers/index.ts --env development",
    "build:worker": "npx wrangler deploy workers/index.ts --env production",
    "build:all": "pnpm build && pnpm build:worker",
    "start-dev": "concurrently \"pnpm dev:worker\" \"pnpm dev\"",
    "dev:watch-css": "tailwindcss -i ./client/src/index.css -o ./client/src/output.css --watch",
    "dev:all": "concurrently \"pnpm dev:worker\" \"pnpm dev:watch-css\" \"pnpm dev\"",
    "prebuild": "node scripts/check-env.js",
    "dev:enhanced": "cross-env VITE_FAST_REFRESH=true FORCE_COLOR=1 concurrently -n \"react,worker,webflow\" -c \"green,blue,magenta\" \"vite --force --clearScreen=false\" \"nodemon --watch workers --ext ts,js --exec 'wrangler dev workers/index.ts --env development'\" \"webflow extension serve\""
  },
