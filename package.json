    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "framer-motion": "^12.4.10",
    "openai": "^4.86.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^3.0.2",
    "zod": "^3.24.2"
  }
