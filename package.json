    "@types/wouter": "^2.2.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@webflow/designer-extension-typings": "*",
    "concurrently": "^9.1.2",
    "lucide-react": "^0.479.0",
    "nodemon": "^3.1.9",
    "react-hook-form": "^7.54.2",
    "ts-node": "^10.9.1",
    "tsx": "^4.19.3",
    "typescript": "*",
    "vite": "^4.4.9",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.0",
    "webflow-cli": "^1.1.0"
