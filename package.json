  "scripts": {
    "build": "vite build && webflow extension bundle",
    "dev": "concurrently -r \"vite --force\" \"webflow extension serve\"",
    "start": "NODE_ENV=production node public/index.js",
    "check": "tsc",
    "dev:worker": "wrangler dev ./workers/index.ts --port 8787 --local",
    "build:worker": "npx wrangler deploy workers/index.ts --env production",
    "build:all": "pnpm build && pnpm build:worker",
    "start-dev": "concurrently \"pnpm dev\" \"pnpm dev:worker\"",
    "dev:watch-css": "./node_modules/.bin/tailwindcss -i ./client/src/index.css -o ./client/src/output.css --watch",
    "dev:all": "concurrently \"pnpm dev:worker\" \"pnpm dev:watch-css\" \"pnpm dev\"",
    "prebuild": "node scripts/check-env.js",
    "test": "vitest run",
    "test:connection": "npx tsx test-connection.ts",
    "dev:setup": "pnpm build && pnpm build:worker",
    "dev:start": "concurrently \"pnpm dev:worker\" \"pnpm dev\"",
    "dev:full": "pnpm dev:setup && pnpm dev:start"
