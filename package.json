{
  "name": "ai-seo-copilot",
  "version": "4.6.3",
  "private": true,
  "main": "client/src/main.tsx",
  "type": "module",
  "scripts": {
    "check": "tsc",
    "lint": "eslint client/src shared workers",
    "test": "vitest run --coverage --root client",
    "test:watch": "vitest --coverage --root client",
    "test:ui": "vitest --ui --coverage --root client",
    "test:bundle": "vitest run src/lib/bundle-validation.test.ts --reporter=verbose --root client",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:unit": "vitest run --coverage --root client src",
    "test:workers": "vitest run --config vitest.workers.config.ts",
    "test:integration": "vitest run --coverage --passWithNoTests tests/integration",
    "test:contract": "vitest run --coverage tests/contract",
    "test:coverage": "vitest run --coverage --reporter=html --root client",
    "test:coverage:threshold": "vitest run --coverage --coverage.thresholds.global.branches=85 --root client",
    "dev": "concurrently -r \"vite --cors --force\" \"vite build --mode development && webflow extension serve\" \"npx wrangler dev ./workers/index.ts --port 8787\"",
    "build": "vite build && webflow extension bundle",
    "deploy:worker": "npx wrangler deploy workers/index.ts --env production",
    "deploy:staging": "npx wrangler deploy workers/index.ts --env staging",
    "deploy:production": "pnpm validate:env && pnpm validate:config && npx wrangler deploy workers/index.ts --env production",
    "validate:config": "vitest run scripts/validate-production-config.test.ts",
    "validate:env": "node scripts/check-env.js",
    "kill-dev": "kill-dev-servers.bat",
    "release": "semantic-release",
    "release:dry": "semantic-release --dry-run",
    "version:update": "node scripts/update-versions.js",
    "changelog": "conventional-changelog -p conventionalcommits -r 2 -i CHANGELOG.md -s",
    "commit": "git-cz --sign"
  },
  "devDependencies": {
    "@anthropic-ai/claude-code": "^2.1.44",
    "@cloudflare/vite-plugin": "^1.11.2",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^10.0.1",
    "@playwright/test": "^1.58.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.4",
    "@tailwindcss/vite": "^4.2.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/adm-zip": "^0.5.7",
    "@types/canvas-confetti": "^1.9.0",
    "@types/cors": "^2.8.19",
    "@types/node": "^24.10.1",
    "@types/node-fetch": "2.6.12",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/sanitize-html": "^2.16.0",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "3.2.4",
    "adm-zip": "^0.5.16",
    "autoprefixer": "^10.4.21",
    "commitizen": "^4.3.1",
    "concurrently": "^9.2.0",
    "conventional-changelog-cli": "^5.0.0",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.25.8",
    "eslint": "^10.0.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "happy-dom": "^20.6.1",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lucide-react": "^0.516.0",
    "msw": "^2.11.2",
    "node-fetch": "3.3.2",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "react-hook-form": "^7.62.0",
    "semantic-release": "^24.2.7",
    "tailwindcss": "^4.1.11",
    "test-data-bot": "^0.8.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.56.0",
    "vite": "^6.3.5",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "wrangler": "^4.28.1"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.4.0",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/postcss": "^4.1.13",
    "@tanstack/react-query": "^5.87.1",
    "@vitejs/plugin-react": "^5.0.2",
    "@webflow/designer-extension-typings": "^2.0.23",
    "@webflow/webflow-cli": "^1.12.2",
    "canvas-confetti": "^1.9.3",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.23.12",
    "hono": "^4.9.7",
    "input-otp": "^1.4.2",
    "net": "^1.0.2",
    "openai": "^5.19.1",
    "react": "^19.1.1",
    "react-day-picker": "^9.10.0",
    "react-dom": "^19.1.1",
    "react-resizable-panels": "^3.0.6",
    "sanitize-html": "^2.17.0",
    "styled-components": "^6.1.19",
    "tailwind-merge": "^3.3.1",
    "undici": "^7.16.0",
    "vaul": "^1.1.2",
    "wouter": "^3.7.1",
    "zod": "^4.1.8"
  },
  "packageManager": "pnpm@10.14.0",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "pnpm": {
    "overrides": {
      "koa": ">=3.0.3",
      "lodash": ">=4.17.23",
      "axios": ">=1.13.5",
      "@conventional-changelog/git-client": ">=2.0.0",
      "minimatch@<3.1.3": "3.1.3",
      "minimatch@>=9.0.0 <9.0.6": "9.0.6"
    }
  }
}
