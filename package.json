{
  "name": "example-webflow-app-react",
  "version": "1.0.0",
  "private": true,
  "main": "client/src/main.tsx",
  "scripts": {
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=public && webflow extension bundle",
    "dev": "concurrently -r \"webflow extension serve\" \"vite build --watch\" \"tsx server/index.ts\"",
    "start": "NODE_ENV=production node public/index.js",
    "check": "tsc"
  },
